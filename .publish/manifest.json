{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.7/MicrosoftTeams.schema.json",
    "manifestVersion": "1.7",
    "version": "{version}",
    "id": "0183acbb-6570-414b-94c2-127079f1c7d3",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Utkarsh",
        "websiteUrl": "{baseUrl0}",
        "privacyUrl": "{baseUrl1}/privacy",
        "termsOfUseUrl": "{baseUrl1}/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "{appname}",
        "full": "{fullappname}"
    },
    "description": {
        "short": "Short description for Tab Demo.",
        "full": "Full description of Tab Demo for Microsoft Teams."
    },
    "accentColor": "#FFFFFF",
    "configurableTabs": [
        {
            "configurationUrl": "{baseUrl1}/config",
            "canUpdateConfiguration": true,
            "scopes": [
                "team",
                "groupchat"
            ]
        }
    ],
    "staticTabs": [
        {
            "entityId": "outlook-tab",
            "name": "Outlook",
            "contentUrl": "{baseUrl2}",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "test-tab",
            "name": "Test Tab",
            "contentUrl": "{baseUrl0}",
            "scopes": [
                "personal"
            ]
        },
        {
            "entityId": "index",
            "name": "Personal Tab",
            "contentUrl": "{baseUrl1}/tab",
            "scopes": [
                "personal"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}